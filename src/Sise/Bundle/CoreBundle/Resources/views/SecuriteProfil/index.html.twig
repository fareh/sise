{% extends '::base.html.twig' %}
{% trans_default_domain 'SiseCoreBundle' %}
{% block body -%}
    {{ render(controller("SiseCoreBundle:Default:menu" , { 'PageContext' : 'List', 'TitleContextValue' :  'SecuriteProfil'|trans() , 'RouteAction' : 'securite_roles' })) }}
    <br>
    <table class="records_list">
        <thead>
            <tr>
                <th>{{'Codeprof'|trans()  }}</th>

                <th>{{'Libeproffr'|trans()  }}</th>


                <th>{{'Libeprofar'|trans()  }}</th>


                <th>{{'Obse'|trans()  }}</th>

                <th>&nbsp;</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('securite_roles_show', { 'id': entity.codeprof }) }}">{{ entity.codeprof }}</a></td>
                <td>{{ entity.libeproffr }}</td>
                <td>{{ entity.libeprofar }}</td>
                <td>{{ entity.obse }}</td>
                <td>
                        <a href="{{ path('securite_roles_edit', { 'id': entity.codeprof }) }}">{{'edit'|trans()  }}</a>

                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('securite_roles_new') }}">
                {{'new'|trans()  }}
            </a>
        </li>
    </ul>
    {% endblock %}
