{% extends '::base.html.twig' %}
{% trans_default_domain 'SiseCoreBundle' %}
{% block body -%}
    {{ render(controller("SiseCoreBundle:Default:menu" , { 'PageContext' : 'List', 'TitleContextValue' :  'SecuriteGroupeutilisateur'|trans(), 'RouteAction' : 'securite_groupeutilisateur' })) }}
<br>
    <table class="records_list">
        <thead>
            <tr>
                <th>{{'Codegrouutil'|trans()  }}</th>
                <th> {{'Libegrouutilfr'|trans()  }}</th>
                <th> {{'Libegrouutilar'|trans()  }}</th>
                <th> {{'Obse'|trans()  }}</th>
                <th>&nbsp;</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('securite_groupeutilisateur_show', { 'id': entity.codegrouutil }) }}">{{ entity.codegrouutil }}</a></td>
                <td>{{ entity.libegrouutilfr }}</td>
                <td>{{ entity.libegrouutilar }}</td>
                <td>{{ entity.obse }}</td>
                <td>

                        <a href="{{ path('securite_groupeutilisateur_edit', { 'id': entity.codegrouutil }) }}">{{ 'edit'|trans() }}</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('securite_groupeutilisateur_new') }}">
             {{ 'new'|trans() }}
            </a>
        </li>
    </ul>
    {% endblock %}
